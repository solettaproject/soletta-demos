{
 "$schema": "http://solettaproject.github.io/soletta/schemas/config.schema",
 "nodetypes": [
  {
   "name": "ScoreLed",
   "type": "gtk/led-7seg"
  },
  {
   "name": "RedTransistor",
   "options": {
    "rgb": {
     "blue": 255
    }
   },
   "type": "gtk/led"
  },
  {
   "name": "YellowTransistor",
   "options": {
    "rgb": {
     "blue": 255
    }
   },
   "type": "gtk/led"
  },
  {
   "name": "RedWinLed",
   "options": {
    "rgb": {
     "red": 255
    }
   },
   "type": "gtk/led"
  },
  {
   "name": "YellowWinLed",
   "options": {
    "rgb": {
     "red": 255,
     "green": 255
    }
   },
   "type": "gtk/led"
  },
  {
   "name": "ScoreTimer",
   "options": {
    "interval": 1000
   },
   "type": "timer"
  },
  {
   "name": "RedScoreCounter",
   "options": {
    "sensor_pin": 0,
    "sensor_device": 0,
    "led_pin": 0
   },
   "type": "ScoreCounter"
  },
  {
   "name": "YellowScoreCounter",
   "options": {
    "sensor_pin": 0,
    "sensor_device": 0,
    "led_pin": 0
   },
   "type": "ScoreCounter"
  }
 ]
}
